    "@storybook/blocks": "^8.3.2",
    "@storybook/react": "^8.3.2",
    "@storybook/react-webpack5": "^8.3.2",
    "@storybook/test": "^8.3.2",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.4",
